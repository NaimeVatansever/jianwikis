<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN"
                            "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="xml">
        <folder name="entities">
            <folder name="org_netbeansrcp_tasklog">
                <file name="TaskLog" url="nbres:/org/netbeans/modules/settings/resources/properties-1_0.dtd">
                    <!--org.netbeansrcp.tasklog.TaskLogTopComponent-->
                    <attr name="hint.originalPublicID" stringvalue="-//org.netbeansrcp.tasklog//TaskLog//EN"/>
                </file>
            </folder>
        </folder>
        <folder name="memory">
            <folder name="org">
                <folder name="netbeansrcp">
                    <folder name="tasklog">
                        <file name="TaskLogTopComponent">
                            <!--org.netbeansrcp.tasklog.TaskLogTopComponent-->
                            <attr name="settings.providerPath" stringvalue="xml/lookups//org_netbeansrcp_tasklog/TaskLog.instance"/>
                        </file>
                    </folder>
                </folder>
            </folder>
        </folder>
        <folder name="lookups">
            <folder name="org_netbeansrcp_tasklog">
                <file name="TaskLog.instance">
                    <!--org.netbeansrcp.tasklog.TaskLogTopComponent-->
                    <attr
                        methodvalue="org.netbeans.api.settings.Factory.create" name="instanceCreate"/>
                    <attr
                        methodvalue="org.netbeans.api.settings.Factory.properties" name="settings.convertor"/>
                    <attr name="settings.instanceClass" stringvalue="org.netbeansrcp.tasklog.TaskLogTopComponent"/>
                    <attr name="settings.instanceOf" stringvalue="org.netbeansrcp.tasklog.TaskLogTopComponent"/>
                    <attr boolvalue="true" name="xmlproperties.preventStoring"/>
                </file>
            </folder>
        </folder>
    </folder>
</filesystem>
