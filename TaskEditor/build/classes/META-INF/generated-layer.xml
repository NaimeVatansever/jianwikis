<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE filesystem PUBLIC "-//NetBeans//DTD Filesystem 1.2//EN"
                            "http://www.netbeans.org/dtds/filesystem-1_2.dtd">
<filesystem>
    <folder name="xml">
        <folder name="entities">
            <folder name="com_netbeansrcp_taskeditor">
                <file name="TaskEditor" url="nbres:/org/netbeans/modules/settings/resources/properties-1_0.dtd">
                    <!--com.netbeansrcp.taskeditor.TaskEditorTopComponent-->
                    <attr name="hint.originalPublicID" stringvalue="-//com.netbeansrcp.taskeditor//TaskEditor//EN"/>
                </file>
            </folder>
        </folder>
        <folder name="memory">
            <folder name="com">
                <folder name="netbeansrcp">
                    <folder name="taskeditor">
                        <file name="TaskEditorTopComponent">
                            <!--com.netbeansrcp.taskeditor.TaskEditorTopComponent-->
                            <attr name="settings.providerPath" stringvalue="xml/lookups//com_netbeansrcp_taskeditor/TaskEditor.instance"/>
                        </file>
                    </folder>
                </folder>
            </folder>
        </folder>
        <folder name="lookups">
            <folder name="com_netbeansrcp_taskeditor">
                <file name="TaskEditor.instance">
                    <!--com.netbeansrcp.taskeditor.TaskEditorTopComponent-->
                    <attr
                        methodvalue="org.netbeans.api.settings.Factory.create" name="instanceCreate"/>
                    <attr
                        methodvalue="org.netbeans.api.settings.Factory.properties" name="settings.convertor"/>
                    <attr name="settings.instanceClass" stringvalue="com.netbeansrcp.taskeditor.TaskEditorTopComponent"/>
                    <attr name="settings.instanceOf" stringvalue="com.netbeansrcp.taskeditor.TaskEditorTopComponent"/>
                    <attr boolvalue="true" name="xmlproperties.preventStoring"/>
                </file>
            </folder>
        </folder>
    </folder>
</filesystem>
